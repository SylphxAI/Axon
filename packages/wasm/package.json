{
  "name": "@sylphx/wasm",
  "version": "0.1.0",
  "type": "module",
  "description": "WASM-accelerated tensor operations",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build:wasm": "asc assembly/index.ts --outFile build/axon.wasm --optimize --runtime stub",
    "build": "bun build src/index.ts --outdir dist --target node --format esm && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist"
  },
  "dependencies": {
    "@sylphx/tensor": "workspace:*"
  },
  "devDependencies": {
    "assemblyscript": "^0.27.0",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "acceleration",
    "tensor-operations"
  ],
  "author": "SylphxAI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SylphxAI/Axon.git",
    "directory": "packages/wasm"
  },
  "homepage": "https://github.com/SylphxAI/Axon#readme",
  "bugs": {
    "url": "https://github.com/SylphxAI/Axon/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
