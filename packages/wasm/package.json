{
  "name": "@neuronline/wasm",
  "version": "0.1.0",
  "type": "module",
  "description": "WASM-accelerated tensor operations for NeuronLine",
  "main": "./dist/index.js",
  "scripts": {
    "build:wasm": "asc assembly/index.ts --outFile build/neuronline.wasm --optimize --runtime stub",
