{
  "name": "@neuronline/nn",
  "version": "3.0.0",
  "type": "module",
  "description": "Pure functional neural network layers for NeuronLine",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "test": "bun test"
  },
  "dependencies": {
    "@neuronline/tensor": "workspace:*",
    "@neuronline/functional": "workspace:*"
  },
  "keywords": [
    "neural-network",
    "layers",
    "pure-functions"
  ],
  "author": "SylphxAI",
  "license": "MIT"
}
